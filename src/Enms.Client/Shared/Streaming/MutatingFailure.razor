@using MudBlazor
@inherits Enms.Client.Base.EnmsComponentBase
<MudDialog>
  <DialogContent>
    @Translate("Failure")
  </DialogContent>
  <DialogActions>
    <MudButton Color="Color.Primary" OnClick="Submit">
      @Translate("Ok")
    </MudButton>
  </DialogActions>
</MudDialog>

@code {

  [CascadingParameter]
  MudDialogInstance MudDialog { get; set; } = default!;

  void Submit()
  {
    MudDialog.Close(DialogResult.Ok(true));
  }

}
